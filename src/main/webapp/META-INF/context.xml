<Context>
<Resource 
	name="mail/Session" auth="Container"
    type="javax.mail.Session"
    mail.smtp.host="localhost"/>
<Resource 
	auth="Container" 
	description="ITSD Datasource" 
	driverClassName="com.mysql.jdbc.Driver" 
	maxActive="50" 
	maxIdle="30" 
	maxWait="10000" 
	name="jdbc/itsd" 
	password="12345678" 
	testOnBorrow="true" 
	testOnReturn="false" 
	testWhileIdle="true" 
	type="javax.sql.DataSource" 
	url="jdbc:mysql://192.168.33.21:3306/contact" 
	username="root" 
	validationInterval="30000" 
	validationQuery="SELECT 1"/>

<Realm className="org.apache.catalina.realm.LockOutRealm" >
<Realm 
	className="org.apache.catalina.realm.JDBCRealm" 
	connectionName="root" 
	connectionPassword="12345678" 
	connectionURL="jdbc:mysql://192.168.33.21:3306/contact" 
	digest="SHA-256" 
	driverName="com.mysql.jdbc.Driver" 
	roleNameCol="role" 
	userCredCol="password" 
	userNameCol="username" 
	userRoleTable="user_roles" 
	userTable="users"/>
</Realm>
</Context>